{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF3084"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDataasetLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDataasetLS'"
		},
		"AzureSynapseAnalytics2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics2'"
		},
		"Azuresqldb_Ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azuresqldb_Ls'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://my2ndadlsstorage16dec.dfs.core.windows.net/"
		},
		"AzureSqlDataasetLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqldb17dec.database.windows.net"
		},
		"AzureSqlDataasetLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb17dec"
		},
		"AzureSqlDataasetLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqldb17decadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF3084_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDataasetLS_password": {
						"value": "[parameters('AzureSqlDataasetLS_password')]"
					},
					"AzureSynapseAnalytics2_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics2_connectionString')]"
					},
					"Azuresqldb_Ls_connectionString": {
						"value": "[parameters('Azuresqldb_Ls_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureSqlDataasetLS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDataasetLS_properties_typeProperties_server')]"
					},
					"AzureSqlDataasetLS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDataasetLS_properties_typeProperties_database')]"
					},
					"AzureSqlDataasetLS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDataasetLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}